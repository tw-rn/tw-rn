{
  "name": "tw-rn",
  "author": "Erick Tamayo",
  "description": "TailwindCSS in React Native",
  "keywords": [
    "tailwindcss",
    "react native",
    "tailwind"
  ],
  "version": "0.0.1-alpha.14",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.16.2",
    "@testing-library/react-native": "^5.0.3",
    "@tw-rn/transformer": "^0.0.1-alpha.14",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.62.12",
    "babel-jest": "^26.1.0",
    "concurrently": "^5.2.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "install-peers-cli": "^2.2.0",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.18.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js --mode production && tsc --emitDeclarationOnly",
    "dev": "concurrently \"npm run dev:webpack\" \"npm run dev:tsc\"",
    "dev:webpack": "webpack --config webpack.config.js --watch --mode development",
    "dev:tsc": "tsc --emitDeclarationOnly --watch",
    "test": "jest",
    "prepare": "install-peers -f"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "fast-memoize": "^2.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/tw-rn/tw-rn",
  "repository": {
    "type": "git",
    "url": "https://github.com/tw-rn/tw-rn"
  },
  "gitHead": "f8a536a46f0cd1ec5dc1c9fc8315cc9139f98309"
}
