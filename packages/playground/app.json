{
  "expo": {
    "name": "playground",
    "slug": "playground",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "default",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "./metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "css"]
    }
  }
}
