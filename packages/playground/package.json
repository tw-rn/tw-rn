{
  "name": "@tw-rn/playground",
  "version": "0.0.13",
  "private": true,
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "dev": "concurrently \"npm run rsync\" \"next\"",
    "rsync": "nodemon --watch ../core --exec \"rsync -rv ../core/* ./node_modules/tw-rn/ --exclude node_modules/ --exclude .jest/ --whole-file\"",
    "build": "next build",
    "start": "expo start",
    "expo:start": "concurrently \"npm run rsync\" \"expo start\"",
    "android": "expo start --android",
    "ios": "concurrently \"npm run rsync\" \"expo start --ios\"",
    "web": "next",
    "eject-next": "next-expo customize",
    "eject": "expo eject",
    "type-check": "tsc",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "expo": "^38.0.0",
    "fast-memoize": "^2.5.2",
    "next": "latest",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.0.tar.gz",
    "react-native-web": "~0.11.7",
    "tw-rn": "^0.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/next-adapter": "2.1.9",
    "@tw-rn/loader": "^0.0.13",
    "@tw-rn/next-adapter": "^0.0.13",
    "@tw-rn/plugin": "^0.0.13",
    "@tw-rn/transformer": "^0.0.13",
    "@types/node": "^14.0.14",
    "@types/react": "~16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-native": "~0.62.13",
    "babel-preset-expo": "^8.2.3",
    "concurrently": "^5.2.0",
    "expo-yarn-workspaces": "^1.2.1",
    "next-transpile-modules": "^3.3.0",
    "nodemon": "^2.0.4",
    "typescript": "~3.9.5"
  }
}
